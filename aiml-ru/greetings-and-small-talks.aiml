<aiml version="1.0.1" encoding="UTF-8">
    <category>
        <pattern> ПРИВЕТ </pattern>
        <template>
            <random>
                <li> Приветствую, человек! </li>
                <li> Добрый день, <get name="name"/> </li>
                <li> Дороу, <get name="name"/> </li>
                <li> Приветик, <get name="name"/> </li>
            </random>
        </template>
    </category>


    <category>
        <pattern> КАК ДЕЛА </pattern>
        <template>
